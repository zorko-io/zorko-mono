{
  "name": "@zorko/server",
  "version": "1.0.4-alpha.0",
  "description": "",
  "author": "",
  "license": "MIT",
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "format": "prettier --write \"src/**/*.ts\"",
    "start": "cross-env NODE_ENV=development ts-node -r tsconfig-paths/register src/main.ts",
    "start:dev": "cross-env NODE_ENV=development nodemon",
    "start:debug": "cross-env NODE_ENV=development nodemon --config nodemon-debug.json",
    "prestart:prod": "rimraf dist && npm run build",
    "clean:dist": "rimraf dist",
    "clean": "npm run clean:dist && rimraf node_modules",
    "start:prod": "node dist/src/main.js",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "seed:start": "cross-env NODE_ENV=development cross-env API_AUTH_ENABLED=false ts-node -r tsconfig-paths/register src/main.ts",
    "seed:init": "ts-node script/load.intit.data.ts"
  },
  "dependencies": {
    "@nestjs/common": "^6.0.0",
    "@nestjs/core": "^6.0.0",
    "@nestjs/jwt": "^6.0.0",
    "@nestjs/mongoose": "^6.1.2",
    "@nestjs/passport": "^6.0.0",
    "@nestjs/platform-express": "^6.0.0",
    "@nestjs/swagger": "^3.0.2",
    "@zorko/client-api": "^1.0.4-alpha.0",
    "@zorko/dto": "^1.0.4-alpha.0",
    "@zorko/remote-api": "^1.0.4-alpha.0",
    "axios": "^0.18.0",
    "bcrypt": "^3.0.6",
    "dotenv": "^8.0.0",
    "lodash": "^4.17.11",
    "mongoose": "^5.5.7",
    "passport": "^0.4.0",
    "passport-http-bearer": "^1.0.1",
    "passport-jwt": "^4.0.0",
    "reflect-metadata": "^0.1.12",
    "rimraf": "^2.6.2",
    "rxjs": "^6.3.3",
    "swagger-ui-express": "^4.0.2"
  },
  "devDependencies": {
    "@nestjs/testing": "^6.0.0",
    "@types/dotenv": "^6.1.1",
    "@types/express": "^4.16.0",
    "@types/faker": "^4.1.5",
    "@types/jest": "^23.3.13",
    "@types/lodash": "^4.14.126",
    "@types/mongoose": "^5.5.0",
    "@types/node": "^10.12.18",
    "@types/supertest": "^2.0.7",
    "@zorko/client-api": "latest",
    "cross-env": "^5.2.0",
    "faker": "^4.1.0",
    "jest": "^23.6.0",
    "nodemon": "^1.18.9",
    "prettier": "^1.15.3",
    "ts-jest": "^23.10.5",
    "ts-node": "^7.0.1",
    "tsconfig-paths": "^3.7.0",
    "tslint": "5.12.1",
    "typescript": "3.4.5",
    "yup": "^0.27.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "gitHead": "d4d95876b722b779104a263a395430f843b492d5"
}
